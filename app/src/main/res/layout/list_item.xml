    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/itemHighlight"
            android:layout_width="6dp"
            android:layout_height="match_parent"
            android:layout_marginRight="2dp"
            android:background="@drawable/feed_item_highlight" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center">

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/listview_Title"
                android:id="@+id/txtTitle"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_columnSpan="2"
                    android:layout_marginLeft="1dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent">

                    <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/listView_Temporary"
                    android:id="@+id/txtStartDate"
                    android:layout_row="1"
                    android:layout_column="1"
                        android:layout_marginLeft="1dp"
                        android:textColor="#797979" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:showDividers="middle">

                    <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="➜"
                    android:id="@+id/txtEndDateLabel"
                    android:layout_row="1"
                    android:layout_column="2"
                    android:textStyle="bold"
                        android:paddingStart="0dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="4dp"
                        android:textColor="#676767" />

                    <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/listView_Temporary"
                    android:id="@+id/txtEndDate"
                    android:layout_row="1"
                    android:layout_column="3"
                        android:textColor="#797979" />
                </LinearLayout>
            </LinearLayout>

            <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/progressDate"
            android:layout_row="3"
            android:layout_column="0"
            android:indeterminate="false"
                android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:max="100"
            android:progress="50"
            android:layout_columnSpan="4"
                android:visibility="gone" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/listView_Date"
                android:id="@+id/txtDateLabel"
                android:layout_row="7"
                android:layout_column="0"
                    android:textStyle="bold"
                    android:layout_marginLeft="1dp"
                    android:textColor="#797979" />

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/listView_Temporary"
                android:id="@+id/txtDate"
                android:layout_row="7"
                android:layout_column="1"
                    android:layout_gravity="fill_horizontal"
                    android:layout_marginLeft="4dp"
                    android:textColor="#797979" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/listView_Description"
                android:id="@+id/txtDescriptionLabel"
                android:layout_row="8"
                android:layout_column="0"
                    android:textStyle="bold"
                android:paddingStart="0dp"
                android:paddingLeft="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/listView_Temporary"
                    android:id="@+id/txtDescription" />


            </LinearLayout>


            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/listView_Coord"
            android:id="@+id/textView4"
            android:layout_row="6"
            android:layout_column="0"
            android:layout_marginTop="5dp"
            android:layout_gravity="fill_horizontal|right"
            android:textStyle="bold"
            android:paddingStart="0dp"
            android:paddingLeft="0dp"
                android:visibility="gone" />

            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/listView_Temporary"
            android:id="@+id/txtCoord"
            android:layout_row="6"
            android:layout_column="1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="fill_horizontal"
                android:visibility="gone" />

            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/listView_Link"
            android:id="@+id/textView3"
            android:layout_row="9"
            android:layout_column="0"
            android:layout_marginTop="5dp"
            android:layout_gravity="fill_horizontal|right"
            android:textStyle="bold"
            android:paddingStart="0dp"
            android:paddingLeft="0dp"
                android:visibility="gone" />

            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/listView_Temporary"
            android:id="@+id/txtLink"
            android:layout_row="9"
            android:layout_column="1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="fill_horizontal"
                android:visibility="gone" />

        </LinearLayout>
    </LinearLayout>

</RelativeLayout>